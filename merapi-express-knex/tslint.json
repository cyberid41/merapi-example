{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            120
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": [
            "debug",
            "info",
            "log",
            "time",
            "timeEnd",
            "trace"
        ]
    },
    "rulesDirectory": []
}